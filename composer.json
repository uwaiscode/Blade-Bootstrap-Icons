{
    "name": "uwaiscode/blade-bootstrapicons",
    "description": "A packages to easly use of bootstrapicons",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "uwaiscode",
            "email": "triyadi@zohomail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "UwaisCode\\BladeBootstrapicons\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "UwaisCode\\BladeBootstrapicons\\BladeBootstrapiconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
